	<!-- 根据条件查询供应商信息 -->
	<select id="queryVender" parameterType="Vender" resultType="Vender">
		select * from vender
		<where>
			<if test="vender_type!=''">
				and vender_type=#{vender_type} 
			</if>
			<if test="zxbj!=''">
				and zxbj=#{zxbj}
			</if>
			<if test="vender_name!=''">
				and vender_name like '%'||#{vender_name}||'%'
			</if>
			<if test="startTime!=''">
				and create_time>=#{startTime}
			</if>
			<if test="endTime!=''">
				<![CDATA[and create_time<=#{endTime}]]>				
			</if>
		</where>
	
	</select>

</mapper>